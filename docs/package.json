{
  "name": "@aivo/docs",
  "version": "2.0.0",
  "private": true,
  "description": "AIVO Developer Documentation Portal",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "gen-api-docs": "docusaurus gen-api-docs all",
    "clean-api-docs": "docusaurus clean-api-docs all",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "test:links": "linkinator ./build --recurse --skip 'https://github.com'",
    "test:a11y": "pa11y-ci --sitemap ./build/sitemap.xml"
  },
  "dependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/plugin-content-docs": "^3.4.0",
    "@docusaurus/plugin-ideal-image": "^3.4.0",
    "@docusaurus/plugin-pwa": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-live-codeblock": "^3.4.0",
    "@docusaurus/theme-mermaid": "^3.4.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.1.0",
    "docusaurus-plugin-openapi-docs": "^3.0.0",
    "docusaurus-theme-openapi-docs": "^3.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.4.0",
    "@docusaurus/tsconfig": "^3.4.0",
    "@docusaurus/types": "^3.4.0",
    "@types/react": "^18.2.0",
    "eslint": "^8.57.0",
    "linkinator": "^6.0.0",
    "pa11y-ci": "^3.1.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
