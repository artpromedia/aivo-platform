{
  "name": "@aivo/ts-shared",
  "version": "1.0.0",
  "description": "Shared TypeScript utilities for AIVO platform including authentication, authorization, and common helpers",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aivo/ts-types": "workspace:*",
    "axios": "^1.7.9",
    "jose": "^6.0.11",
    "ioredis": "^5.6.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.19",
    "@nestjs/core": "^10.4.19",
    "@tanstack/react-query": "^5.80.6",
    "@types/express": "^5.0.3",
    "@types/node": "^22.15.21",
    "fastify": "^5.3.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "fastify": "^5.x",
    "express": "^4.x || ^5.x",
    "@tanstack/react-query": "^5.x",
    "@nestjs/common": "^10.x",
    "@nestjs/core": "^10.x",
    "rxjs": "^7.x"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "directory": "libs/ts-shared"
  },
  "keywords": [
    "aivo",
    "authentication",
    "jwt",
    "authorization",
    "middleware"
  ],
  "license": "UNLICENSED"
}
