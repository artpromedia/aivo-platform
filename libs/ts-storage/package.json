{
  "name": "@aivo/ts-storage",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./service": {
      "import": "./dist/storage.service.js",
      "types": "./dist/storage.service.d.ts"
    },
    "./scanner": {
      "import": "./dist/virus-scanner.js",
      "types": "./dist/virus-scanner.d.ts"
    },
    "./access": {
      "import": "./dist/access-control.js",
      "types": "./dist/access-control.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "vitest run",
    "build": "tsc -p tsconfig.json",
    "prepare": "pnpm build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.679.0",
    "@aivo/ts-types": "workspace:*",
    "@aivo/ts-rbac": "workspace:*"
  },
  "devDependencies": {
    "@prisma/client": "^6.0.0",
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  }
}
