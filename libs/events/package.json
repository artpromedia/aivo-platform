{
  "name": "@aivo/events",
  "version": "1.0.0",
  "type": "module",
  "description": "AIVO Event Schemas, Types, and Publisher for NATS JetStream",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "require": "./dist/schemas/index.cjs"
    }
  },
  "files": [
    "dist",
    "schemas"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/schemas/index.ts --format cjs,esm --dts --clean --tsconfig tsconfig.build.json",
    "dev": "tsup src/index.ts src/schemas/index.ts --format cjs,esm --dts --watch --tsconfig tsconfig.build.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "nats": "^2.28.0",
    "zod": "^3.23.8",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "aivo",
    "events",
    "nats",
    "jetstream",
    "streaming"
  ],
  "license": "UNLICENSED",
  "private": true
}
