{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noImplicitAny": false,
    "strictNullChecks": false,
    "exactOptionalPropertyTypes": false,
    "verbatimModuleSyntax": false,
    "noUncheckedIndexedAccess": false,
    "useUnknownInCatchVariables": false,
