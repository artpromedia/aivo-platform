# Session Service Environment Variables

# Server port
PORT=4020

# PostgreSQL connection string
DATABASE_URL="postgresql://postgres:password@localhost:5432/aivo_sessions?schema=public"

# JWT public key path for token verification
JWT_PUBLIC_KEY_PATH="../auth-svc/keys/jwt-public.pem"

# Log level: trace, debug, info, warn, error
LOG_LEVEL=info
