{
  "name": "@aivo/analytics-svc",
  "version": "0.0.1",
  "description": "Analytics service for Homework & Focus metrics",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate",
    "etl": "tsx src/etl/cli.ts",
