{
  "name": "@aivo/content-svc",
  "version": "0.0.1",
  "description": "Learning Object content management service",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src test --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@aivo/ts-rbac": "workspace:*",
    "@aivo/ts-storage": "workspace:*",
    "@aivo/ts-types": "workspace:*",
    "@aivo/ts-utils": "workspace:*",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.1",
    "@prisma/client": "^5.22.0",
    "fastify": "^4.28.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  }
}
