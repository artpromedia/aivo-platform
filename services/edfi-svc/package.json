{
  "name": "@aivo/edfi-svc",
  "version": "1.0.0",
  "description": "Ed-Fi Data Standard integration service for state reporting",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "^1.7.0",
    "cron": "^3.1.0",
    "fastify": "^4.29.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "22.19.1",
    "prisma": "5.22.0",
    "tsx": "^4.19.0",
    "typescript": "5.9.3",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": "20.19.4"
  }
}
